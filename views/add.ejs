<%- include('header0'); -%>
<h1>Add new user</h1>
<form class = "form-signin" action="/add" method="POST" enctype="multipart/form-data">
  <div class="form-group">
    <label>Name</label>
    <input type="text" name="name" autocomplete="off" class = "form-control">
    <input type="hidden" name="gameUrl" value="<%= value %>"></input>
    <!-- Choose files -->
    <label>Upload your photo</label>
    <input type="file" id="file" name="img" accept="image/*">
  <button type="submit" name="button" class = "btn btn-primary">Post</button>
</form>

</body>
</html>
